    "@nebular/eva-icons": "12.0.0",
    "@nebular/security": "12.0.0",
    "@nebular/theme": "12.0.0",
    "@swimlane/ngx-charts": "^20.5.0",
    "bootstrap": "4.3.1",
    "chart.js": "^4.4.4",
    "ckeditor": "4.7.3",
    "classlist.js": "1.1.20150312",
    "core-js": "2.5.1",
    "echarts": "^5.5.1",
    "eva-icons": "^1.1.3",
    "intl": "1.2.5",
    "ionicons": "2.0.1",
    "leaflet": "1.2.0",
